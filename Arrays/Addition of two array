import java.util.*

fun main() {
   val EvenArr = arrayOf(0, 2, 4, 6, 8)
   val OddArr = arrayOf(1, 3, 5, 7, 9)

   OddArr[0] += oddArr[0]
   OddArr[1] += oddArr[1]
   OddArr[2] += oddArr[2]
   OddArr[3] += oddArr[3]
   OddArr[4] += oddArr[4]

   println(Arrays.toString(OddArr))
}

/* 
OUTPUT
[1, 5, 9, 13, 17]
*/
